.contain__body {
  position: relative;
  padding-top: 55px;
  box-sizing: border-box;
}

.contain__search {
  background: #46C1FD;
  padding: 10px;
  width: 100%;
  box-sizing: border-box;
  position: fixed;
  top: 0;
  left: 0;
}

.input__search {
  width: 100%;
  background: #ffffff;
  padding: 0 1.3rem;
  height: 35px;
  line-height: 35px;
  box-sizing: border-box;
  font-size: .85rem;
  border-radius: 16px;
}

.style__input-placeholder {
  font-size: .85rem;
}

.result-title {
  padding: .125rem .325rem;
  font-size: .8rem;
  background: #f1f3f5;
  color: #313335;
}

.contain__result-empty {
  width: 100%;
  height: 100%;
  display: flex;
}

.contain__result-empty view {
  margin: auto;
  padding:2rem 1.3rem;
}

.contain__result-empty view text {
  display: block;
  font-size: .8rem;
  margin-bottom: .125rem;
  color: #666870;
}