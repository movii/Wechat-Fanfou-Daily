page {
  background: #f1f3f5;
}

.footer {
  min-height: 70px;
  background: #fefefe;
  box-sizing: border-box;
  display: flex;
  flex-flow: column nowrap;
  align-items: center; 
  justify-content: center; 
  padding: 1.325rem 0;
}

.footer .logo{
  width: 35px;
  height: 35px;
  margin-bottom: .65rem;
  border-radius: 50%;
  border: 1px solid #cccccc;
  background: #e1e1e1;
  background-image: url(data:image/png;base64,);
  background-position: center;
  background-repeat: no-repeat;
  background-size: contain;
}

.footer .logo__text {
  display: flex;
  align-items: center;
  font-size: .65rem;
  color: #676869;
}

.footer .logo__text text {
  font-size: .65rem;
  color: #4DC2FA;
  margin-left: .325rem;
}
